{
  "extends": "./tsconfig.isomorphic.json",
  "compilerOptions": {
     "rootDir": "./tmp-source-dist-prod",
     "outDir": "dist-prod",
    "module": "ESNext",
     "target": "ESNext",
     # "isolatedModules": true, maybe later
     "importsNotUsedAsValues": "remove",
     "noEmitHelpers": true
  },
  "include": [
    "tmp-source-dist-prod/**/*"
  ],
  "exclude": [
    "tmp-source-dist-prod/assets"
  ]
}
