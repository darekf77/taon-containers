{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false,
    "schematicCollections": [
      "@cypress/schematic",
      "@schematics/angular"
    ]
  },
  "newProjectRoot": "projects",
  "projects": {
    "angular-electron": {
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "configurations": {
            "dev": {
              "aot": false,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dev.ts"
                }
              ],
              "namedChunks": false,
              "optimization": false,
              "outputHashing": "none",
              "sourceMap": true
            },
            "production": {
              "aot": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "namedChunks": false,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false
            }
          },
          "options": {
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "browser": "src/main.ts",
            "index": "src/index.html",
            "inlineStyleLanguage": "scss",
            "outputPath": {
              "base": "dist"
            },
            "polyfills": [
              "zone.js"
            ],
            "scripts": [],
            "styles": [
              "src/styles.scss"
            ],
            "tsConfig": "src/tsconfig.app.json"
          }
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "development": {
              "buildTarget": "angular-electron:build:dev"
            },
            "production": {
              "buildTarget": "angular-electron:build:production"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "buildTarget": "angular-electron:build"
          }
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "browsers": [
              "chromium"
            ],
            "buildTarget": "angular-electron:build:testing",
            "coverage": true,
            "coverageReporters": [
              "html",
              "lcovonly"
            ],
            "reporters": [
              [
                "default",
                {
                  "summary": true
                }
              ],
              [
                "html",
                {
                  "outputFile": "test-results/vitest/index.html"
                }
              ]
            ],
            "runner": "vitest",
            "tsConfig": "src/tsconfig.spec.json"
          }
        }
      },
      "projectType": "application",
      "root": "",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        },
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "sourceRoot": "src"
    },
    "app": {
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "configurations": {
            "development": {
              "extractLicenses": false,
              "optimization": false,
              "sourceMap": true
            },
            "production": {
              "budgets": [],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "serviceWorker": "ngsw-config.json"
            }
          },
          "defaultConfiguration": "production",
          "options": {
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
            "baseHref": "/",
            "browser": "src/main.ts",
            "index": "src/index.html",
            "inlineStyleLanguage": "scss",
            "outputMode": "server",
            "outputPath": {
              "base": "dist/app",
              "browser": "browser",
              "server": "server"
            },
            "preserveSymlinks": true,
            "scripts": [],
            "server": "src/main.server.ts",
            "ssr": {
              "entry": "src/server.ts"
            },
            "styles": [
              "src/styles.scss"
            ],
            "tsConfig": "tsconfig.app.json"
          }
        },
        "ct": {
          "builder": "@cypress/schematic:cypress",
          "configurations": {
            "development": {
              "devServerTarget": "app16:serve:development"
            }
          },
          "options": {
            "devServerTarget": "app16:serve",
            "headless": false,
            "testingType": "component",
            "watch": true
          }
        },
        "cypress-open": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "headless": false,
            "watch": true
          }
        },
        "cypress-run": {
          "builder": "@cypress/schematic:cypress",
          "configurations": {
            "production": {
              "devServerTarget": "app16:serve:production"
            }
          },
          "options": {
            "devServerTarget": "app16:serve"
          }
        },
        "e2e": {
          "builder": "@cypress/schematic:cypress",
          "configurations": {
            "production": {
              "devServerTarget": "app16:serve:production"
            }
          },
          "options": {
            "devServerTarget": "app16:serve",
            "headless": false,
            "watch": true
          }
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "development": {
              "buildTarget": "app:build:development"
            },
            "production": {
              "buildTarget": "app:build:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
            "inlineStyleLanguage": "scss",
            "karmaConfig": "karma.conf.js",
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "scripts": [],
            "styles": [
              "src/styles.scss"
            ],
            "tsConfig": "tsconfig.spec.json"
          }
        }
      },
      "projectType": "application",
      "root": "",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        },
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "sourceRoot": "src"
    },
    "my-lib": {
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "development": {
              "tsConfig": "projects/my-lib/tsconfig.lib.json"
            },
            "production": {
              "tsConfig": "projects/my-lib/tsconfig.lib.prod.json"
            }
          },
          "defaultConfiguration": "production",
          "options": {
            "project": "projects/my-lib/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "tsConfig": "projects/my-lib/tsconfig.spec.json"
          }
        }
      },
      "prefix": "lib",
      "projectType": "library",
      "root": "projects/my-lib",
      "sourceRoot": "projects/my-lib/src"
    }
  },
  "version": 1
}
